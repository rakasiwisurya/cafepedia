{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "off",
            "windows"
        ],
        "eol-last": [
            "off",
            "always"
        ],
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-undef": "off",
        "no-restricted-globals": [
            "off",
            "event",
            "fdescribe"
        ],
        "import/extensions": [
            "off"
        ]
    }
}